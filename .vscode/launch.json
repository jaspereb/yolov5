{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "train testing",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "python.pythonPath": "~/git/yolov5/virtualenv/bin",
            "args": [
                "--img",
                "640",
                "--data",
                "coco.yaml",
                "--device",
                "cpu",
                "--cfg",
                "dual.yaml",
                "--batch",
                "1",
                "--weights",
                "yolov5l.pt"
            ]
        },
        {
            "name": "dual train testing",
            "type": "python",
            "request": "launch",
            "program": "train.py",
            "console": "integratedTerminal",
            "python.pythonPath": "~/git/yolov5/virtualenv/bin",
            "args": [
                "--img",
                "640",
                "--data",
                "BCDP-mini.yaml",
                "--device",
                "cpu",
                "--hyp",
                "hyp.noAugment.yaml",
                "--cfg",
                "dual.yaml",
                "--batch",
                "1",
                "--weights",
                "yolov5l.pt"
            ]
        },
        {
            "name": "dual test testing",
            "type": "python",
            "request": "launch",
            "program": "val.py",
            "console": "integratedTerminal",
            "python.pythonPath": "~/git/yolov5/virtualenv/bin",
            "args": [
                "--img",
                "320",
                "--data",
                "BCDP.yaml",
                //"--device",
                //"cpu",
                "--batch",
                "1",
                "--weights",
                "models/best.pt",
                "--task",
                "test",
                "--save-txt"
                //"--save-hybrid",
                //"--save-conf"
            ]
        }
    ]
}