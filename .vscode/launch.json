{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "train testing",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "python.pythonPath": "~/git/yolov5/virtualenv/bin",
            "args": [
                "--img",
                "640",
                "--data",
                "BCDP-mini.yaml",
                "--device",
                "cpu",
                "--hyp",
                "data/hyps/hyp.noAugment.yaml",
                "--cfg",
                "yolov5l.yaml",
                "--batch",
                "1",
                "--weights",
                "yolov5l.pt"
            ]
        }
    ]
}